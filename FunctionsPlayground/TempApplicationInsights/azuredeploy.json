{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location for all resources."
      }
    },
    "applicationInsightsName": {
      "type": "string",
      "defaultValue": "FunctionsPlaygroundAI",
      "metadata": {
        "description": "Application Insights name."
      }
    }
  },
  "variables": {
    "appInsName": "[parameters('applicationInsightsName')]"
  },
  "resources": [
    {
      "apiVersion": "2014-04-01",
      "name": "[variables('appInsName')]",
      "type": "Microsoft.Insights/components",
      "location": "[parameters('location')]",
      "properties": {
        "ApplicationId": "[variables('appInsName')]"
      }
    }
  ],
  "outputs": {
    "MyAppInsightsInstrumentationKey": {
      "value": "[reference(resourceId('Microsoft.Insights/components', parameters('applicationInsightsName')), '2014-04-01').InstrumentationKey]",
      "type": "string"
    }
  } 

}
